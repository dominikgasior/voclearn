{
  "root": "libs/api/revision/application",
  "sourceRoot": "libs/api/revision/application/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/api/revision/application/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/api/revision/application"],
      "options": {
        "jestConfig": "libs/api/revision/application/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:application"]
}
