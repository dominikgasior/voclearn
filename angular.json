    "voclearn": {
      "projectType": "application",
      "root": "apps/voclearn",
      "sourceRoot": "apps/voclearn/src",
      "prefix": "voclearn",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/voclearn",
            "index": "apps/voclearn/src/index.html",
            "main": "apps/voclearn/src/main.ts",
            "polyfills": "apps/voclearn/src/polyfills.ts",
            "tsConfig": "apps/voclearn/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/voclearn/src/favicon.ico",
              "apps/voclearn/src/assets"
            ],
            "styles": ["apps/voclearn/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/voclearn/src/environments/environment.ts",
                  "with": "apps/voclearn/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "voclearn:build:production"
            },
            "development": {
              "browserTarget": "voclearn:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/voclearn/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "voclearn:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/voclearn/src/**/*.ts",
              "apps/voclearn/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/voclearn"],
          "options": {
            "jestConfig": "apps/voclearn/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "voclearn-e2e": {
      "root": "apps/voclearn-e2e",
      "sourceRoot": "apps/voclearn-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/voclearn-e2e/cypress.json",
            "devServerTarget": "voclearn:serve:development",
            "tsConfig": "apps/voclearn-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "voclearn:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/voclearn-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["voclearn"]
